{
  "presets": [
    [
      "next/babel",
      {
        "styled-jsx": { "optimizeForSpeed": false }
      }
    ]
  ],
  "plugins": [
    "relay",
    [
      "import",
      {
        "libraryName": "antd"
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "react-native": "react-native-web"
        }
      }
    ]
  ]
}
