    "react-native-paper": "^3.10.1",
    "react-native-vector-icons": "oblador/react-native-vector-icons",
    "react-native-web": "^0.12.3",
    "react-quill": "^1.3.1",
    "react-relay": "^9.0.0",
    "relay-runtime": "^9.0.0"
