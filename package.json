  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "relay": "relay-compiler --src ./ --schema schema.graphql",
    "relayw": "relay-compiler --src ./ --schema schema.graphql --watch",
    "schema": "./node_modules/.bin/fetch-schema http://web.thecommunity.ng/_/api -f schema.graphql",
    "schema:local": "./node_modules/.bin/fetch-schema http://localhost:5000/_/api -f schema.graphql",
    "start": "NODE_ENV=production node server.js -p $PORT",
    "heroku-postbuild": "next build",
    "wstart": "set NODE_ENV=production&&node server.js -p $PORT"
  },
