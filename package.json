{
  "name": "thecommunity",
  "version": "1.0.0",
  "description": "A publishing platform",
  "main": "index.js",
  "author": "Emmanuel Nosakhare::TheCommunity <nonreactiv@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "node server.js",
    "build": "next build",
    "relay": "relay-compiler --src ./ --schema schema.graphql --persist-ouput queryMap.json",
    "relayw": "relay-compiler --src ./ --schema schema.graphql --watch --persist-ouput queryMap.json",
    "schema": "./node_modules/.bin/fetch-schema https://web.thecommunity.ng/_/api -f schema.graphql",
    "schema:local": "./node_modules/.bin/fetch-schema http://localhost:5000/_/api -f schema.graphql",
    "start": "NODE_ENV=production node server.js -p $PORT",
    "postinstall": "node ./postinstall",
    "wstart": "set NODE_ENV=production&&node server.js -p $PORT"
  },
  "dependencies": {
    "@shoutem/ui": "nosakharegroup/ui",
    "@weco/next-plugin-transpile-modules": "^2.0.2",
    "@zeit/next-sass": "^1.0.1",
    "antd": "^3.6.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "express": "^4.16.3",
    "isomorphic-unfetch": "^3.0.0",
    "next": "9.1.2",
    "next-images": "^0.10.5",
    "next-offline": "^4.0.2",
    "next-routes": "^1.4.2",
    "node-sass": "^4.10.0",
    "nprogress": "^0.2.0",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-native-web": "^0.11.4",
    "react-quill": "^1.3.1",
    "react-relay": "^7.0.0",
    "relay-runtime": "^7.0.0"
  },
  "devDependencies": {
    "@types/react": "^16.8.0",
    "@types/react-native": "^0.57.62",
    "babel-cli": "^6.26.0",
    "babel-plugin-import": "^1.8.0",
    "babel-plugin-relay": "^7.0.0",
    "fetch-schema": "^0.0.14",
    "graphql": "^14.5.8",
    "prettier": "^1.18.2",
    "relay-compiler": "^7.0.0",
    "tslint": "^5.11.0",
    "typescript": "^3.6.4"
  },
  "engines": {
    "node": "10",
    "yarn": "1.22.0"
  }
}
