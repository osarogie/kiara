{
  "name": "thecommunity",
  "version": "1.0.0",
  "description": "A publishing platform",
  "main": "index.js",
  "author": "Emmanuel Nosakhare::TheCommunity <nonreactiv@gmail.com>",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "relay": "relay-compiler --src ./ --schema schema.graphql",
    "relayw": "relay-compiler --src ./ --schema schema.graphql --watch",
    "schema": "./node_modules/.bin/fetch-schema https://hq.thecommunity.ng/api/v2/graphql -f schema.graphql",
    "schema:local": "./node_modules/.bin/fetch-schema http://localhost:5000/api/v2/graphql -f schema.graphql",
    "start": "next start -p $PORT",
    "postinstall": "node ./postinstall",
    "export": "cross-env NEXT_EXPORT=true next build && cross-env NEXT_EXPORT=true next export"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^9.3.4",
