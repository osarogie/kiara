{
  "name": "kiara",
  "env": {
    "SITE_HOST": "@tc-site-host",
    "SECURE_HOST": "@tc-secure-host",
    "API_HOST": "@tc-api-host"
  },
  "routes": [
    {
      "src": "^/service-worker.js$",
      "dest": "/_next/static/service-worker.js",
      "headers": {
        "cache-control": "public, max-age=43200, immutable",
        "Service-Worker-Allowed": "/"
      }
    }
  ],
  "builds": [
    {
      "src": "next.config.js",
      "use": "@now/next"
    }
  ],
  "version": 2
}
